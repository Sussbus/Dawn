{
  "name": "Dawn",
  "version": "1.0.0",
  "description": "A simple way of making commands",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.bbb.dawn",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS"
   }
  },
  "repository": "https://github.com/Sussbus/Dawn",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.9",
    "electron-builder": "^19.46.9"
  },
  "dependencies": {
    "menubar": "^5.2.3",
    "vex-js": "^4.0.1"
  }
}
